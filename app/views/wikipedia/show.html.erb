

<h1>
  <% if @won == true %>
    YOU WON!!!!
  <% elsif render_cheater == true%>
    YOU CHEATED!!!!
  <% else %>
    <%= game_move_count_funny_message %>
  <% end %>
</h1>

<div class="wikipedia_container_header">


<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h3>Starting Point:  <%= current_game_start_name %>
        <%= render 'imageinfo', url_location: current_game.start_point %>
        <br>
        <span class="end_display"> Ending Point: <%= current_game_end_name %>
          <%= render 'imageinfo', url_location: current_game.end_point %>
        </span>
      </h3>
    </div>
      <div class="col-md-6">
        <span class="pull-right">
        <h3>  Current Number of Moves: <%= game_move_count %>
          <br>
          Game Timer: <%= gametimer(current_game)%></h3>
        </span>
      </div>
    </div>
</div>
</div>
<%= render 'showwikipage', game: @game %>
